<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="ida:AADInstance" value="https://login.windows.net/{0}" />
    <add key="ida:Tenant" value="[Enter tenant name, e.g. contoso.onmicrosoft.com]" />
    <add key="ida:ClientId" value="[Enter client ID as obtained from Azure Portal, e.g. 82692da5-a86f-45c9-9d53-2f88d51b478b]" />
    <add key="ida:CertName" value="[Enter subject name of self-signed certificate you created with makecert.exe, e.g. CN=TodoListDaemonWithCert]" />
    <add key="todo:TodoListResourceId" value="[Enter App ID URI of TodoListService, e.g. https://contoso.onmicrosoft.com/TodoListService]" />
    <add key="todo:TodoListBaseAddress" value="https://localhost:44321" />
    <add key="Test" value="Test" />
    
  </appSettings>
</configuration>